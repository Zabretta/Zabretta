{
  "name": "samodelkin",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "check": "npm run lint && npm run build",
    "preview": "npm run build && npm run start",
    "clean": "rm -rf .next node_modules",
    "reinstall": "npm run clean && npm install",
    "open:dev": "npm run dev && (xdg-open http://localhost:3000 || echo 'Open http://localhost:3000 in browser')",
    "studio": "npx @next/bundle-analyzer"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "@types/node": "^20.19.30",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/socket.io-client": "^3.0.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5"
  }
}
